<?xml version="1.0" encoding="UTF-8" ?>
<ldml>
	<dates>
		<symbols>
			<dateSeparator>.</dateSeparator>
		</symbols>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="short">
						<dateFormat >
							<pattern>d.MM.yyyy</pattern>
						</dateFormat>
					</dateFormatLength >
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="long">
						<timeFormat >
							<pattern>H:mm:ss</pattern>
						</timeFormat>
					</timeFormatLength >
				</timeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>