<?xml version="1.0" encoding="UTF-8" ?>
<ldml>
  <numbers>
    <decimalFormats>
      <decimalFormatLength >
        <decimalFormat >
          <pattern>#,##0.###;-#,##0.###</pattern>
        </decimalFormat>
      </decimalFormatLength >
    </decimalFormats>
  </numbers>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="short">
						<dateFormat >
							<pattern>dd/MM/yyyy</pattern>
						</dateFormat>
					</dateFormatLength >
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="long">
						<timeFormat >
							<pattern>h:mm:ss tt</pattern>
						</timeFormat>
					</timeFormatLength >
				</timeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>