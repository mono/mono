    <HttpHandlerPackageVersionPrefix>$(HttpHandlerPackageMajorVersion).$(HttpHandlerPackageMinorVersion).$(HttpHandlerPackagePatchVersion)</HttpHandlerPackageVersionPrefix>
    <HttpHandlerPackageVersion>$(HttpHandlerPackageVersionPrefix)-$(HttpHandlerPackagePreReleaseVersionLabel)</HttpHandlerPackageVersion>
    <PreviousHttpHandlerPackageReleaseVersion Condition=" '$(HttpHandlerPackagePatchVersion)' != '0' ">$(HttpHandlerPackageMajorVersion).$(HttpHandlerPackageMinorVersion).$([MSBuild]::Subtract($(HttpHandlerPackagePatchVersion), 1))</PreviousHttpHandlerPackageReleaseVersion>
    <ClientWebSocketPackageMajorVersion>0</ClientWebSocketPackageMajorVersion>
    <ClientWebSocketPackageMinorVersion>1</ClientWebSocketPackageMinorVersion>
    <ClientWebSocketPackagePatchVersion>0</ClientWebSocketPackagePatchVersion>
    <ClientWebSocketPackagePreReleasePreviewNumber>1</ClientWebSocketPackagePreReleasePreviewNumber>
    <ClientWebSocketPackagePreReleaseVersionLabel>preview$(ClientWebSocketPackagePreReleasePreviewNumber)</ClientWebSocketPackagePreReleaseVersionLabel>
    <ClientWebSocketPackagePreReleaseBrandingLabel>Preview $(ClientWebSocketPackagePreReleasePreviewNumber)</ClientWebSocketPackagePreReleaseBrandingLabel>
    <ClientWebSocketPackageMajorMinorVersion>$(ClientWebSocketPackageMajorVersion).$(ClientWebSocketPackageMinorVersion)</ClientWebSocketPackageMajorMinorVersion>
    <ClientWebSocketPackageVersionPrefix>$(ClientWebSocketPackageMajorVersion).$(ClientWebSocketPackageMinorVersion).$(ClientWebSocketPackagePatchVersion)</ClientWebSocketPackageVersionPrefix>
    <ClientWebSocketPackageVersion>$(ClientWebSocketPackageVersionPrefix)-$(ClientWebSocketPackagePreReleaseVersionLabel)</ClientWebSocketPackageVersion>
    <PreviousClientWebSocketPackageReleaseVersion Condition=" '$(ClientWebSocketPackagePatchVersion)' != '0' ">$(ClientWebSocketPackageMajorVersion).$(ClientWebSocketPackageMinorVersion).$([MSBuild]::Subtract($(ClientWebSocketPackagePatchVersion), 1))</PreviousClientWebSocketPackageReleaseVersion>
  </PropertyGroup>

