<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Mono.WebAssembly runtime libraries. This package is not intended to be referenced directly.</Description>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <!-- get rid of warnings about assemblies not in lib -->
    <NoPackageAnalysis>true</NoPackageAnalysis>
  </PropertyGroup>
  <ItemGroup>
    <None Update="build\netstandard2.0\Mono.WebAssembly.Runtime.props" Pack="True" />
    <Content Include="..\..\..\wasm\debug\mono.js" PackagePath="mono-wasm-runtime\%(Filename)%(Extension)" Link="mono-wasm-runtime\mono.js" />
    <Content Include="..\..\..\wasm\debug\mono.wasm" PackagePath="mono-wasm-runtime\%(Filename)%(Extension)" Link="mono-wasm-runtime\mono.wasm" />
  </ItemGroup>
</Project>