    <Member MemberName="AddNewItem">
      <MemberSignature Language="C#" Value="public System.Windows.Forms.ToolStripItem AddNewItem { set; get; }" />
      <MemberType>Property</MemberType>
      <AssemblyInfo>
        <AssemblyVersion>2.0.0.0</AssemblyVersion>
