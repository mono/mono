      <Attributes>
        <Attribute>
          <AttributeName>System.Configuration.ConfigurationProperty("maxLimit", DefaultValue="2147483647")</AttributeName>
        </Attribute>
        <Attribute>
          <AttributeName>System.Configuration.IntegerValidator(MinValue=0, MaxValue=2147483647)</AttributeName>
