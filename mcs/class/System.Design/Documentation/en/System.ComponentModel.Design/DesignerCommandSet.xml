    <Member MemberName="GetCommands">
      <MemberSignature Language="C#" Value="public virtual System.Collections.ICollection GetCommands (string name);" />
      <MemberType>Method</MemberType>
      <AssemblyInfo>
        <AssemblyVersion>2.0.0.0</AssemblyVersion>
